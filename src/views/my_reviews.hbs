{{#each reviews}}
    <div class="each_review">
        <div>location: {{this.location_name}} </div>
        <div>place reviewed: {{this.place}} </div>
        <img src="{{this.img_url}}" alt="">
        <div>review: {{this.review}} </div>
        <div>by: {{this.user_fname}} {{this.user_lname}}</div>
        <button><a href="/edit/{{this.location_id}}">Edit Review</a></button>
    </div>
{{/each}}


<script src="/js/my_reviews.js"></script>
